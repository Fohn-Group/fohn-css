{
  "name": "fohn-ui-css",
  "version": "1.3.0",
  "description": "Tailwind css utilities for Fohn-Ui php framework",
  "main": "dist/fohn-ui.min.css",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build-dist": "NODE_ENV=production postcss ./src/fohn-defaut.css -o dist/fohn-ui.min.css",
