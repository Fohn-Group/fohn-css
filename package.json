  "license": "MIT",
  "homepage": "http://www.fohn-ui.com/",
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "autoprefixer": "^10.4.12",
    "cssnano": "^6.0.0",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
    "tailwindcss": "^3.2.1"
  },
  "dependencies": {
    "vue-toastification": "^2.0.0-rc.5"
  }
}
